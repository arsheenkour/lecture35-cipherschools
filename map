#include<iostream>
#include<map>
using namespace std;
int main()
{
	map<string,int>hash;
	hash["rohit"]=1;
	hash["programming"]=2;
	hash["reddy"]=3;
	cout<<"the value of rohit"<<hash["c++"];
	for(auto j=hash.begin();j!=hash.end();j++)
	
	{
		cout<<"the key will be"<<j->first<<"the value in the key will be"<<j->second<<endl;
		
	}
}